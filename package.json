{
  "name": "react-image-publisher",
  "version": "0.0.3",
  "description": "A common blockchain React component for publishing images with Bitstore and Open Publish",
  "main": "dist.js",
  "scripts": {
    "build:src": "babel src/index.js > dist.js",
    "build:example": "browserify ./example/example.js > ./example/dist.js",
    "build": "npm run build:src && npm run build:example",
    "test": "npm run build && tapes test/**/*.js | tap-spec",
    "start": "http-server ./example/ -a 0.0.0.0 -p 9000 -c-1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockai/react-image-publisher.git"
  },
  "keywords": [
    "react",
    "common-blockchain",
    "bitcoin",
    "bitstore",
    "openpublish",
    "image",
    "publish",
    "react-component"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blockai/react-image-publisher/issues"
  },
  "homepage": "https://github.com/blockai/react-image-publisher#readme",
  "dependencies": {
    "bitstore": "0.0.5",
    "openpublish": "0.0.6",
    "shasum": "^1.0.1"
  },
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "bitcoinjs-lib": "^1.5.7",
    "blockcast": "^1.0.3",
    "browserify": "^10.2.6",
    "blockcypher-unofficial": "^1.4.1",
    "file-api": "^0.10.4",
    "filereader": "^0.10.3",
    "http-server": "^0.8.0",
    "jsdom": "^3.1.2",
    "randombytes": "^2.0.1",
    "react-tools": "^0.13.3",
    "request": "^2.58.0",
    "tap-spec": "^4.0.2",
    "tapes": "^2.0.0"
  }
}
